{
  "extends": "airbnb-base",
  "plugins": [
    "vue"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "commonjs": true,
    "meteor": true,
    "mongo": true,
    "jquery": true,
    "amd": true
  },
  "parser": "babel-eslint",
  "rules": {
    "vue/jsx-uses-vars": 2,
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "react/prop-types": 0,
    "indent": [
      "error",
      4
    ],
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "global-require": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "forOf": true,
      "jsx": true,
      "es6": true
    }
  },
  "globals": {
    "ensure": true,
    "global": true,
    "windows": true
  }
}
